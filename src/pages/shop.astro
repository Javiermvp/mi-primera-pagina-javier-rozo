---
import Naranja from "../components/Naranja.astro";

import Layout from "../layouts/Layout.astro"
---

<Layout>
    <div>
        <h1>COLECCION DE MARVEL</h1>
        <Naranja title="hulk" price={15}img="/hulk.png" />
        <Naranja title=" spiderman" price={10}img="/spiderman.png"  />
        <Naranja title="capitan america" price={25}img="/capitanamerica.png"  />
        <Naranja title="thanos" price={30}img="/thanos.png"  />
        <Naranja title="starlord" price={15}img="/starlord.png"  />
        <h1>COLECCION DE ANIME</h1>
        <Naranja title="demon" price={20}img="/demon.png"  />
        <Naranja title="naruto" price={15}img="/naruto.png"  />
        <Naranja title="onepiece" price={25}img="/onepiece.png"  />
        <Naranja title="zoro" price={30}img="/zoro.png"  />
        <Naranja title="dragonballz" price={10}img="/dragonballz.png"  />
        <h1>COLECCION DE VIDEOJUEGOS</h1>
        <Naranja title="godofwar" price={20}img="/godofwar.png"  />
        <Naranja title="mario" price={15}img="/mario.png"  />
        <Naranja title="pacman" price={25}img="/pacman.png"  />
        <Naranja title="scorpion" price={30}img="/scorpion.png"  />
        <Naranja title="sonic" price={10}img="/sonic.png"  />

    </div>
</Layout>


<style>
    div {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(300px,1fr));
    }
</style>